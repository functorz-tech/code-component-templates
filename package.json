{
  "name": "code-component-template",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gen-gql": "graphql-codegen",
    "watch:gql": "npm-watch gen-gql",
    "watch:build": "npm-watch build"
  },
  "watch": {
    "gen-gql": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "gql"
      ]
    },
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "ts",
        "tsx"
      ]
    }
  },
  "dependencies": {
    "antd": "^5.21.3",
    "classnames": "^2.5.1",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "echarts-gl": "^2.0.9",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zvm-code-context": "^0.4.10"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.3.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-codegen/typescript-resolvers": "^4.3.0",
    "@originjs/vite-plugin-federation": "^1.3.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.9.0",
    "npm-watch": "^0.13.0",
    "sass": "^1.79.4",
    "typescript": "5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "gitHead": "b05bca03eceea45f80c27e52d76f385cfb2ffd5d"
}